[http]
	version = HTTP/1.1
[user]
	name = bekaboo
	email = kankefengjing@gmail.com
[init]
	defaultBranch = master
[alias]
	a    = add
	ap   = add -p
	au   = add -u
	aup  = add -up
	b    = branch
	bd   = branch -d
	bf   = branch --force
	bdf  = branch -d --force
	bdr  = push -d
	bl   = branch -v
	bu   = branch -u
	brn  = branch -m
	bn   = checkout -b
	bs   = bisect
	bsb  = bisect bad
	bsg  = bisect good
	bsr  = bisect reset
	bss  = bisect start
	c    = commit
	ca   = commit --amend
	ce   = commit --amend --no-edit
	cl   = clone
	cl1  = clone --depth=1
	cln  = clone --filter=blob:none
	clr  = clone --recursive
	cm   = commit -m
	co   = checkout
	cob  = checkout -b
	cof  = checkout --force
	; Checkout specified commit without changing the working tree and branch
	; pointer, aka "soft checkout"
	cos  = "!fn() { git checkout --detach && git reset --soft $1 && git checkout $1; }; fn"
	d    = diff
	dt   = difftool
	ds   = diff --cached
	f    = fetch
	ia   = merge-base --is-ancestor
	l    = log --oneline --graph
	lg   = log
	; Show git commit history for a single line/range, see
	; https://stackoverflow.com/a/50473741
	ll   = log -L
	lr   = reflog
	; `$GIT_PREFIX` contains the relative path from repo root to current working
	; directory with trailing slash
	; When no argument is given to `git lc`, use repo root as base path
	; (default behavior) and all files in the repo is counted,
	; else use current shell cwd as base path and only count files in given path
	lc   = "!fn() { git ls-files -z ${1:+\"$GIT_PREFIX$1\"} | xargs -0 wc -l 2>/dev/null; }; fn"
	lca  = "!fn() { git ls-files -z ${1:+\"$GIT_PREFIX$1\"} | xargs -0 cat | wc -l 2>/dev/null; }; fn"
	ls   = ls-files
	m    = merge
	ma   = merge --abort
	mc   = merge --continue
	mt   = mergetool
	p    = push
	pf   = push --force
	pu   = push -u
	puf  = push -u --force
	pfu  = push -u --force
	pk   = cherry-pick
	pka  = cherry-pick --abort
	pkc  = cherry-pick --continue
	pl   = pull
	r    = remote
	ra   = remote add
	rb   = rebase
	rba  = rebase --abort
	rbc  = rebase --continue
	rbh  = rebase --onto HEAD
	rbi  = rebase -i
	rbo  = rebase --onto
	rl   = remote -v
	rd   = remote rm
	rrm  = remote rm
	rrn  = remote rename
	rs   = reset
	rss  = reset --soft
	rsm  = reset --mixed
	rsh  = reset --hard
	rt   = restore
	rtp  = restore -p
	rts  = restore --staged
	rtsp = restore --staged -p
	ru   = remote set-url
	rv   = revert
	rva  = revert --abort
	rvc  = revert --continue
	rvs  = revert --skip
	s    = stash
	sa   = stash apply
	sb   = stash branch
	sd   = stash drop
	sl   = stash list
	sm   = stash -m
	sp   = stash pop
	ss   = stash --staged
	ssm  = stash --staged -m
	st   = status
	su   = submodule
	sui  = submodule init
	sul  = submodule status
	suu  = submodule update
	sw   = stash show
	swi  = switch
	swc  = switch --create
	swd  = switch --detach
	swo  = switch --orphan
	sww  = stash show -p
	t    = tag
	ta   = tag -a
	td   = tag -d
	tl   = tag -l
	tm   = tag -m
	tc   = tag --contains
	w    = show --stat
	wa   = log --diff-filter=A
	wc   = log --diff-filter=C
	wd   = log --diff-filter=D
	wm   = log --diff-filter=M
	wp   = log --diff-filter=B
	wr   = log --diff-filter=R
	wt   = log --diff-filter=T
	wu   = log --diff-filter=U
	ww   = show
	wx   = log --diff-filter=X
[merge]
	tool = vimdiff
[mergetool]
	keepBackup = false
[mergetool "vimdiff"]
	cmd = nvim -d $LOCAL $MERGED $REMOTE +'wincmd l'
[pull]
	rebase = true
[diff]
	algorithm = histogram
	indentHeuristic = true
[filter "vscode-theme"]
	smudge = cat
	clean = sed \
		-e '/[cC]olorTheme/d' \
		-e '/defaultInterpreterPath/d'
[filter "kglobalshortcutsrc"]
	smudge = cat
	clean = sed '/switch-to-activity-/d'
[filter "kwinrc"]
	smudge = cat
	; Filter out lines containing UUID or changing values
	clean = sed \
		-e '/Latitude.*=/d' \
		-e '/Longitude.*=/d' \
		-e '/Id_[[:digit:]]\\+=/d' \
		-e '/^\\[Activities\\]/,/^$/d' \
		-e '/^\\[SubSession:.*\\]/,/^$/d' \
		-e '/^\\[Tiling\\]\\[[[:alnum:]-]\\+\\]/,/^$/d'
[filter "okularpartrc"]
	smudge = cat
	clean = sed \
		-e '/^\\[Main View\\]/,/^$/d' \
		-e '/^\\[Nav Panel\\]/,/^$/d' \
		-e '/ChangeColors=/d'
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[include]
	path = .gitconfig.local
[push]
	default = current
	autoSetupRemote = true
