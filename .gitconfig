[http]
	version = HTTP/1.1
[user]
	name = bekaboo
	email = 18127878294@qq.com
[init]
	defaultBranch = master
[alias]
	a = add
	b = branch
	c = commit
	cm = commit -m
	ca = commit -a
	cam = commit -a -m
	d = diff
	l = log --oneline --graph
	pl = pull
	ps = push
	st = status
	ss = stash
	ssm = stash -m
	ssp = stash pop
	ssa = stash apply
	rs = reset
	rt = restore
	rts = restore --staged
	rb = rebase
	rbp = rebase -i HEAD~5
	lc = !git ls-files -z | xargs -0 wc -l
[merge]
	tool = vimdiff
	ff = false
[mergetool]
	keepBackup = false
[mergetool "vimdiff"]
	cmd = nvim -d $LOCAL $REMOTE $MERGED -c '$wincmd w' -c 'wincmd J'
[pull]
	rebase = true
[diff]
	algorithm = patience
